<div class="card card-info card-inverse">
  <h1 class="card-header">Import Members</h1>
  <div class="card-block">    
    <% @aclx_files.spreadsheets.each do |sheet| %>
      <div class="row">
        <div class="col-xs-6">
          <span><%= sheet.title %></span>
        </div>
        <div class="col-xs-6">
          <%= form_tag import_users_path, multipart: true do %>
            <%= hidden_field_tag :file, sheet.human_url %>
            <%= submit_tag "Select", class: "btn btn-default", target: "_blank" %>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
</div>